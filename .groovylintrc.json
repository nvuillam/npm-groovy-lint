{
    "extends": "recommended",
    "rules": {
        "CatchThrowable": {
            "enabled": false
        },
        "ClassJavadoc": {
            "enabled": false
        },
        "ClosureAsLastMethodParameter": {
            "enabled": false
        },
        "DuplicateNumberLiteral": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "ExplicitArrayListInstantiation": {
            "enabled": false
        },
        "ExplicitLinkedListInstantiation": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },
        "PrintStackTrace": {
            "enabled": false
        },
        "PropertyName": {
            "enabled": false
        },
        "SpaceAroundMapEntryColon": {
            "enabled": false
        },
        "SystemExit": {
            "enabled": false
        },
        "UnnecessaryGetter": {
            "enabled": false
        },
        "UnnecessarySetter": {
            "enabled": false
        },
        "VariableName": {
            "enabled": false
        },
        "VariableTypeRequired": {
            "enabled": false
        }
    }
}